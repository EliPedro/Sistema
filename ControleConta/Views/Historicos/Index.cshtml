@model IEnumerable<ControleConta.Models.Historico>

@{
    ViewBag.Title = "Index";
}


<table class="table table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cedente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sacado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Valor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cedente)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sacado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Valor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Data)
        </td>
        <td>
        </td>
    </tr>
}

</table>

